{
  "name": "react-bolt",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://boltsite.github.io/",
  "dependencies": {
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "cra-template": "1.2.0",
    "emailjs-com": "^3.2.0",
    "framer-motion": "^12.4.4",
    "gh-pages": "^6.3.0",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.7",
    "react-dom": "^19.0.0",
    "react-helmet-async": "^2.0.5",
    "react-responsive": "^10.0.0",
    "react-router-dom": "^7.1.1",
    "react-router-sitemap": "^1.2.0",
    "react-scripts": "5.0.1",
    "react-snap": "^1.23.0",
    "react-transition-group": "^4.4.5",
    "styled-components": "^6.1.14",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "sitemap": "babel-node src/sitemapGenerator.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "postbuild": "react-snap"
  },
  "reactSnap": {
    "source": "./dist",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/",
      "/about",
      "/company/company01",
      "/company/company02",
      "/contact",
      "/products/products01/menu01",
      "/products/products01/menu02",
      "/products/products01/menu03",
      "/products/products02/menu01",
      "/products/products02/menu02",
      "/products/products02/menu03",
      "/products/products02/menu04",
      "/products/products02/menu05",
      "/products/products02/menu06",
      "/products/products02/menu07",
      "/products/products02/menu08",
      "/products/products03/menu01",
      "/products/products03/menu02",
      "/products/products03/menu03",
      "/products/products03/menu04",
      "/products/products04/menu01",
      "/products/products04/menu02",
      "/products/products04/menu03"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0"
  }
}